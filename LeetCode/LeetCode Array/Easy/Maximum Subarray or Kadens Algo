#Maximum Subarray or Kadens Algo

def kadens_algo(arr):
    current_sum = arr[0]
    max_sum = arr[0]
    
    start,best_start,best_end = 0
    
    for i in arr[1:]:
        current_sum = max(i,current_sum+i)
        max_sum = max(current_sum,max_sum)
    
    return max_sum
    
print(kadens_algo([-2, 1, -3, 4, -1, 2, 1, -5, 4]))

#Time Complexity - O(n) 
