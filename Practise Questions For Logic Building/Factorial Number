//Factorial Number Print



function factorial(n){
    let result = 1;
    
    for(let i = n; i >= 1; i--){
        result *= i
    }
    return result;
}

console.log(factorial(0)); // 1
console.log(factorial(2)); // 2
console.log(factorial(3)); // 6
console.log(factorial(4)); //24



//Time Complexity Big-O = O(n) (Linear Time Complexity) 
// Single Loop is used
